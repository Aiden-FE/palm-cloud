{
  "pages": [
    {
      "path": "pages/resources/resources",
      "style": {
        "navigationStyle": "custom",
        "backgroundTextStyle": "light",
        "navigationBarTitleText": "文件",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录"
      }
    }
  ],
  "tabBar": {
    "color": "#374151",
    "selectedColor": "#0791F8",
    "backgroundColor": "#FFF",
    "borderStyle": "white",
    "spacing": "5px",
    "iconWidth": "20px",
    "list": [
      {
        "pagePath": "pages/resources/resources",
        "text": "文件",
        "iconPath": "static/images/folder.png",
        "selectedIconPath": "static/images/folder_selected.png"
      },
      {
        "pagePath": "pages/my/my",
        "text": "我的",
        "iconPath": "static/images/profile.png",
        "selectedIconPath": "static/images/profile_selected.png"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uv-(.*)": "@climblee/uv-ui/components/uv-$1/uv-$1.vue"
    }
  }
}
